{
  "name": "@counter-monorepo/types",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "@counter-monorepo/source": "./src/index.ts",
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./counter-ui-native": {
      "@counter-monorepo/source": "./src/counter-ui-native.tsx",
      "types": "./dist/counter-ui-native.d.ts",
      "import": "./dist/counter-ui-native.js",
      "default": "./dist/counter-ui-native.js"
    },
    "./counter-ui-web": {
      "@counter-monorepo/source": "./src/counter-ui-web.tsx",
      "types": "./dist/counter-ui-web.d.ts",
      "import": "./dist/counter-ui-web.js",
      "default": "./dist/counter-ui-web.js"
    }
  },
  "nx": {
    "sourceRoot": "packages/types/src",
    "targets": {
      "build": {
        "executor": "@nx/js:swc",
        "outputs": [
          "{options.outputPath}"
        ],
        "options": {
          "outputPath": "packages/types/dist",
          "main": "packages/types/src/index.ts",
          "tsConfig": "packages/types/tsconfig.lib.json",
          "skipTypeCheck": true,
          "stripLeadingPaths": true
        }
      }
    }
  },
  "dependencies": {
    "@swc/helpers": "~0.5.11",
    "react-native": "0.79.3"
  },
  "peerDependencies": {
    "react": "^19.0.0"
  }
}
